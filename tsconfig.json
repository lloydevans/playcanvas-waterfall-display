{
  "compilerOptions": {
    "lib": ["ES2020", "DOM"],
    "module": "ES2015",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noUnusedParameters": false,
    "removeComments": true,
    "sourceMap": false,
    "strict": false,
    "strictFunctionTypes": true,
    "strictNullChecks": false,
    "importHelpers": true,
    "target": "ES5"
  }
}
